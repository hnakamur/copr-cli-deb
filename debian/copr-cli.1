.sp
.if n \{\
.RS 4
.\}
.nf
copr\-cli build myproject some\&.src\&.rpm
copr\-cli build someone_else/project some\&.src\&.rpm
copr\-cli build \-r fedora\-24\-x86_64 \-r fedora\-24\-i386 "@somegroup/project" some\&.src\&.rpm
.fi
.if n \{\
.RE
.\}
